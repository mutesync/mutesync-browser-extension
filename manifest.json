{
  "name": "mutesync-browser-extension",
  "description": "Extends mutesync desktop app to web clients.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
  ],
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "content_script.js"
      ],
      "matches": [
        "https://meet.google.com/*"
      ],
      "exclude_matches": [
        "https://meet.google.com/getalink"
      ],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_title": "m√ºtesync",
    "default_icon": "icon-128.png",
    "default_popup": "popups/default.html"
  },
  "key": ""
}
